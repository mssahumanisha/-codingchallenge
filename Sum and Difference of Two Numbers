#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{ 
	int a,b;
    float c,d;
    scanf("%d %d",&a,&b);
    scanf("%f %f",&c,&d);
    int sum=0,sub=0;
    float sum1=0,sub1=0;
    sum=a+b;
    sub=a-b;
    sum1=c+d;
    sub1=c-d;
    
    printf("%d %d\n",sum,sub);
     printf("%0.1f %0.1f",sum1,sub1);
    
    return 0;
}
